---
import * as Content from "astro:content"

import Base from "../../layouts/base.astro"
import AppsList from "../../components/apps_list.astro"

let packaged = await Content.getCollection("metadata", ({ data }) => data.app_type == "packaged")
---

<Base title="All Packaged Apps">
	<link slot="imports" rel="stylesheet" href="/lists.css">

	<AppsList apps={ packaged }/>
</Base>